{
  "name": "@jmjhox/date-simplify",
  "version": "2.0.0",
  "private": false,
  "author": {
    "name": "JMJHOX",
    "email": "josemiguelaparicio507@gmail.com"
  },
  "types": "types/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "dist/date-simplify"
  ],
  "scripts": {
    "lint": "eslint --ext .tsx,.ts,.js .",
    "typescript": "tsc --noEmit",
    "test:date-simplify": "jest --coverage ",
    "prebuild": "del lib/ types/",
    "babel": "babel src --out-dir lib --extensions '.tsx,.ts,.js' --ignore '**/tests/**' --source-maps --delete-dir-on-start",
    "build:dev": "tsc --jsx react --noEmit",
    "build:ci": "tsc --jsx react"
  },
  "dependencies": {
    "date-fns": "2.28.0",
    "core-js": "3.6.5",
    "jasmine": "4.0.2",
    "ts-node": "10.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/config-conventional": "^8.3.4",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/jest": "^26.0.0",
    "codecov": "^3.7.0",
    "commitlint": "^8.3.5",
    "del-cli": "^3.0.1",
    "eslint": "^7.2.0",
    "eslint-config-satya164": "^3.1.7",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "karma-typescript": "^5.5.3",
    "prettier": "^2.0.5",
    "release-it": "^13.6.2",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JMJHOX/date-simplify.git"
  },
  "keywords": [
    "date",
    "date-time",
    "time",
    "date-simplify",
    "simplified-date",
    "simplied",
    "date",
    "calculator",
    "ng",
    "angular",
    "format",
    "node",
    "react",
    "typescript",
    "javascript",
    "library"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JMJHOX/date-simplify/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/types/",
      "<rootDir>/lib/"
    ],
    "testRegex": "/tests/.*\\.(test|spec)\\.(js|tsx?)$"
  },
  "eslintConfig": {
    "extends": "satya164"
  },
  "eslintIgnore": [
    "node_modules/",
    "coverage/",
    "lib/"
  ],
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "browserslist": [
    ">1%",
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not android <= 4.4",
    "not samsung <= 4"
  ]
}
